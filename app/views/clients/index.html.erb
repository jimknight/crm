<h2>Clients</h2>
<p><%= link_to 'New Client', new_client_path, :class => "btn btn-primary" %></p>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= link_to client.name, client %></td>
        <td><%= client.phone %></td>
      </tr>
    <% end %>
  </tbody>
</table>
